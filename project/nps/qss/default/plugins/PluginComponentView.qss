ToolPage QWidget {
	background-color: #FFFFFF;
}


/* list*/
MyListView {
	border-top: none;
	border-width: 1px;
	border-color: #CECECE;
}

MyListView {
	padding-top: 4px;
}

/* search line*/

SearchLineEdit QPushButton#iconButton {
	border: none;
	background-color: transparent;
	border-image: url(:/PluginComponentView/fangda);
	width: 16px;
	height: 16px;
	margin: 0px 0px 0px 0px;
}

SearchLineEdit QPushButton#clearButton {
	border: none;
	background-color: #B7CADE;
	background-image: url(:/PluginComponentView/close);
	width: 16px;
	height: 16px;
	margin: 0px 0px 0px 0px;
}

SearchLineEdit QPushButton#clearButton:hover {
	background-color: #D1E0EF;
}

SearchLineEdit QPushButton#clearButton:pressed {
	background-color: #7E9CBA;
}

/* search line*/
SearchLineEdit {
border:1px solid #CECECE;
}

SearchLineEdit QPushButton#iconButton{
border:none;
background-color:transparent;
border-image:url(:/PluginComponentView/fangda);
width:16px;
