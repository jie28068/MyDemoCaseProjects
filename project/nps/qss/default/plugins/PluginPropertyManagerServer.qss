/*QCheckBox*/
QCheckBox::indicator{
	width:20px;
	height:20px;
	background:transparent;
}

QCheckBox::indicator:enabled:checked{
	image:url(:/checkbox/Resources/CheckBox_checked.png);
} 

QCheckBox::indicator:enabled:unchecked{
	image:url(:/checkbox/Resources/CheckBox_unchecked.png);
} 

QCheckBox::indicator:enabled:indeterminate{
	image:url(:/checkbox/Resources/CheckBox_partiallyChecked.png);
} 

QCheckBox::indicator:!enabled:checked{
	image:url(:/checkbox/Resources/CheckBox_checked_disable.png);
} 

QCheckBox::indicator:!enabled:unchecked{
	image:url(:/checkbox/Resources/CheckBox_unchecked_disable.png);
} 

QCheckBox::indicator:!enabled:indeterminate{
	image:url(:/checkbox/Resources/CheckBox_partiallyChecked_disable.png);
} 

QWidget#PropertyDialog{
	background-color: #fbfcff;
}

PropertyDialog QLabel{
	qproperty-alignment: 'AlignVCenter | AlignLeft';
}

PropertyDialog QLabel#Iconlabel{
	qproperty-alignment: 'AlignVCenter | AlignHCenter';
}

QLabel#PortListTittle{
	max-height: 25px;
	qproperty-alignment: 'AlignVCenter | AlignLeft';
}

QComboBox#DrawBoardNameComb, #ElecTypeComb, #ElecElementComb{
	border-width: 0px;
}

PowerFlowSetWidget QTabWidget::pane,PropertyDialog QTabWidget::pane {
	border-top: 1px solid #E1E8EF;
	padding-top: 10px;
}

PowerFlowSetWidget QTabWidget QTabBar,PropertyDialog QTabWidget QTabBar{
	font:normal bold 14px "Microsoft YaHei";
}
PowerFlowSetWidget QTabWidget QTabBar::tab,PropertyDialog QTabWidget QTabBar::tab{
	min-height:30px;
	min-width:30px;
	border-top:0px solid #bfbfbf;
	border-right:0px solid #bfbfbf;
	border-bottom:2px solid #E1E8EF;
}
PowerFlowSetWidget QTabWidget QTabBar::tab:first,PropertyDialog QTabWidget QTabBar::tab:first
{
	border-left:1px solid #bfbfbf;
}

PowerFlowSetWidget QTabWidget QTabBar::tab:selected,PropertyDialog QTabWidget QTabBar::tab:selected{
	color:#003d74;
	background-color: #FFFFFF;
	border-bottom-color:#003d74;
}
PowerFlowSetWidget QTabWidget QTabBar::tab:hover:!selected,PropertyDialog QTabWidget QTabBar::tab:hover:!selected{
	border-bottom-color:transparent;
}

PropertyDialog > QStackedWidget{
	border: 1px solid #bfbfbf;
	padding-left: 10px;
	padding-right:10px;
	padding-bottom: 10px;
}

PropertyDialog > QStackedWidget >QWidget {
	border: 0px;
	padding: 0px;
	margin:0px;
}

PropertyDialog QStackedWidget QComboBox#SwitchWidgetTabBox {
	font:normal bold 14px "Microsoft YaHei";
