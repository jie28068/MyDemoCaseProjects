	border-left:none;
	border-right:none;
	border-bottom:1px solid #E1E8EF;
}

PropertyDialog > QStackedWidget >QMainWindow {
	border: 0px;
	padding: 0px;
	margin:0px;
}

PropertyDialog > QWidget#leftwidget {
	border: 0px solid #bfbfbf;
}

PropertyDialog[isDlgmemoryBlock="true"] > QWidget#leftwidget > QTreeWidget {
	border: 0px solid #bfbfbf;
	margin-left: 0px;
	background-color: #fbfcff;
}

PropertyDialog[isDlgmemoryBlock="false"] > QWidget#leftwidget > QTreeWidget {
	border: 1px solid #bfbfbf;
	margin-left: 0px;
	background-color: #fbfcff;
}

PropertyDialog > QWidget#leftwidget > QTreeWidget QTreeWidgetItem {
	height:25px;
	border: none;
}

PropertyDialog > QWidget#leftwidget > QTreeWidget::item::hover {
	height:25px;
	border: none;
	margin-right:0px;
}

PropertyDialog > QWidget#leftwidget > QTreeWidget::item::selected {
